<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1,maximum-scale=1,user-scalable=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <title>全班合格</title>
    <link rel="stylesheet" href="../../css/api.css" />
    <link rel="stylesheet" href="../../css/commonWindow.css" />
    <style type="text/css">
        body{
            min-width: 320px;
        }
        .select-date{
            width:100%;
            height:42px;
            padding-left:8px;
            padding-right:8px;
            padding-top:8px;
            box-sizing: border-box;
            position:relative;
            z-index:2;
        }
        .select-date .sd{
            width:100%;
            height:42px;
            background: #FFFFFF;
            box-shadow: 0 2px 4px 0 #CDE3E9;
            border-radius: 4px;
            position: relative;
            text-align: center;
        }
        .sd .left-btn>img, .sd .right-btn>img{
            width:16px;
            height:16px;
        }
        .sd .left-btn{
            position:absolute;
            left:15px;
            top:13.5px;
        }
        .sd .right-btn{
            position:absolute;
            right:15px;
            top:13.5px;
        }
        .sd .show-time{
            font-size: 16px;
            color: #333333;
            line-height:42px;
        }
        .pie-chart{
            width:100%;
            padding-left:8px;
            padding-right:8px;
            margin-top:20px;
            box-sizing: border-box;
        }
        .pc{
            width:100%;
            height:141px;
            background: #FFFFFF;
            box-shadow: 0 2px 4px 0 #CDE3E9;
            border-radius: 8px;
            padding-top:12px;
            padding-left:40px;
            position: relative;
            box-sizing: border-box;
        }
        .pc>canvas{
            width:234px;
            height:117px;
        }
        .blank{
            width:50px;
            height:50px;
            background: #FFFFFF;
            border-radius:50%;
            position:absolute;
            top:46.5px;
            left:75px;
        }
        .right-tag{
            position: absolute;
            top:35px;
            left:181px;
        }
        .normal>:nth-child(1){
            width:16px;
            height:16px;
            background: #67C49C;
            box-shadow: 0 2px 4px 0 rgba(155,218,191,0.50);
            border-radius: 2px;
        }
        .normal{
            position:relative;
        }
        .unnormal{
            margin-top:6px;
            position:relative;
        }
        .unnormal-rate{
            margin-top:6px;
            position:relative;
        }
        .unnormal>:nth-child(1){
            width:16px;
            height:16px;
            background: #D4602C;
            box-shadow: 0 2px 4px 0 rgba(155,218,191,0.50);
            border-radius: 2px;
        }
        .unnormal-rate>:nth-child(1){
            width:16px;
            height:16px;
            background: #FFFFFF;
        }
        .normal>:nth-child(1),.unnormal>:nth-child(1),.unnormal-rate>:nth-child(1){
            position: absolute;
            top:3px;
        }
        .normal>:nth-child(2),.unnormal>:nth-child(2),.unnormal-rate>:nth-child(2){
            margin-left:28px;
            font-size:12px;
            color:#333333;
        }
        .normal>:nth-child(3),.unnormal>:nth-child(3),.unnormal-rate>:nth-child(3){
            margin-left:12px;
            font-size:16px;
            color:#151515;
        }
        .detail-list{
            width:100%;
            padding:12px 8px 19px 8px;
            box-sizing: border-box;
        }
        .dl{
            width:100%;
            background: #FFFFFF;
            box-shadow: 0 2px 4px 0 #CDE3E9;
            padding:12px 12px 0px;
            border-radius: 8px;
            box-sizing: border-box;
        }
        .dl-title ul li{
            width:50%;
            height:26px;
            line-height:26px;
            font-size: 14px;
            color: #999999;
            float:left;
            text-align: center;
            box-sizing: border-box;
        }
        .dl-title ul .active{
            color:#5EC4E0 !important;
        }
        .dl-title ul>:nth-child(1){
            border-right:1px solid #E8E7E7;
        }
        .dl-info{
            clear:left;
            /* margin-top:8px; */
            box-sizing: border-box;
            padding-top:12px;
        }
        .dl-info ul{
            width:100%;
            box-sizing: border-box;
        }
        .dl-info .student-list li{
            width:100%;
            height:41px;
            border-bottom:1px solid #F2F2F2;
            font-size: 12px;
            color:#333333;
            box-sizing: border-box;
        }
        .student-list li div{
            width:50%;
            height:41px;
            line-height:41px;
            float:left;
            box-sizing: border-box;
        }
        .student-list li>:nth-child(1){
            padding-left:16px;
        }
        .student-list li>:nth-child(2){
            padding-left:22px;
        }
        .info-title{
            width:100%;
            height:44px;
            background: #EBFBFF;
            border-radius:4px;
        }
        .info-title li{
            width:50%;
            height:44px;
            float:left;
            box-sizing: border-box;
            line-height:44px;
            font-size: 14px;
            color: #333333;
            letter-spacing: 0.19px;
        }
        .info-title>:nth-child(1){
            padding-left:16px;
        }
        .dl-info .student-list>:nth-last-child(1){
            border:none;
        }
        #topbar>:nth-child(2)>span>img{
            margin-left:6px;
        }
        .status-list{overflow:hidden;}
        .dialog-bg{
            width:100%;
            height:100%;
            background: rgba(0,0,0,0.30);
            position:fixed;
            top:0;left:0;
            z-index:99999999
        }
        .dialog-info{
            width:60%;
            position:fixed;
            top:50%;
            left:50%;
            transform:translateX(-50%) translateY(-50%);
            background: #fff;
            box-shadow: 0 0 6px 0 rgba(90,200,225,0.30);
            border-radius: 4px;
            padding:14px 20px 30px;
            box-sizing: border-box;
        }
        .info-list li{
            width:100%;
            height:44px;
            line-height:43px;
            text-align: left;
            font-size: 14px;
            color: #666666;
            letter-spacing: 0.29px;
            overflow: hidden;
            text-overflow: ellipsis;
            white-space: nowrap;
            /* box-sizing: border-box; */
        }
        .btm-line{
            width:100%;
            height:auto;
            padding-left:7px;
            padding-right:7px;
        }
        .btm-line>div{
            width:100%;
            height:1px;
            background: #F0F0F0;
        }
        .ensure{
            width:50%;
            height:44px;
            margin:20px auto 0;
            background: #5ac8e1;
            text-align: center;
            line-height:44px;
            font-size:16px;
            color:#FFFFFF;
            border-radius: 4px;
        }
        .main{padding:8px;background:#fff;margin-top:10px;}
        .main button{
            width:100%;height:44px;
            background: #5AC8E1;
            box-shadow: 0 2px 4px 0 rgba(90,200,225,0.30);
            border-radius: 4px;
            color:#fff;outline:none
        }
    </style>
</head>
<body>
<div id="topbar" >
    <div id="leftButton" tapmode="itemOn" onclick="closeWin()">
        <img style="width:30px;height:30px;" src="../../icon/back.png">
    </div>
    <div id="CYangName"><span></span></div><!--<img src="../../icon/arrowup.png" alt="">-->
</div>
<div class="select-date">
    <div class="sd">
        <div class="left-btn dtpicker-compButton increment dtpicker-compButtonEnable" onclick="Subtract()"><img src="../../icon/arrowPre.png" alt=""></div>
        <div class="show-time">
            <span class="year-month" id="Time"></span>
            <span class="week" id="week"></span>
        </div>
        <div class="right-btn dtpicker-compButton decrement dtpicker-compButtonEnable" onclick="increase()"> <img src="../../icon/arrowNext.png" alt=""></div>
    </div>
</div>
<div class="main">
    <button id="Ylovesc" onclick="qualified()"></button>
</div>

<div id="dialogYc" class="dialog-bg" style="display:none;">
    <div class="dialog-info">
        <ul class="info-list">
            <li class="class-name">
                <span id="YangName"></span>
                <div class="btm-line">
                    <div></div>
                </div>
            </li>
            <li class="check-result">
                <span>晨检合格记录：</span>
                <span>合格</span>
                <div class="btm-line">
                    <div></div>
                </div>
            </li>
        </ul>
        <div class="ensure" onclick="Yqualified()">确定</div>
    </div>
</div>
</body>
<script type="text/javascript" src="../../script/api.js"></script>
<script type="text/javascript" src="../../script/jquery-1.8.1.min.js"></script>
<script type="text/javascript" src="../../script/mui.min.js"></script>
<script type="text/javascript" src="../../script/util.js"></script>
<script type="text/javascript" src="../../script/user.js"></script>
<script type="text/javascript" src="../../script/kv.js"></script>
<script type="text/javascript" src="../../script/apiCloud.js"></script>
<script type="text/javascript" src="../../script/public.js"></script>
<script type="text/javascript" src="../../script/template-native.js"></script>
<script type="text/javascript" src="../../script/data.js"></script>
<script>
    var cid;
    var butAction = false;
    apiready = function (){
        cid = getUserInfo().cids[0];
        var Yday = formatTimeToDay_day(Date.parse(document.getElementById('Time').innerHTML));
        var Cname = getUserInfo().clazzes[0].name;
        document.getElementById("CYangName").innerHTML = Cname;
        document.getElementById("YangName").innerHTML = Cname;
        document.getElementById("Ylovesc").innerHTML = Cname + "  晨检合格记录";
        document.getElementById('topbar').style.paddingTop = api.safeArea.top + 'px';
        document.getElementById('topbar').style.display = "block";
        ajaxGet(getClazzAllDayCheckByDay,{cid:cid,day:Yday},function(ret,err){
      
            if(ret.success == false){
                    var Ylovesc = document.getElementById("Ylovesc");
                    Ylovesc.setAttribute("disabled", true);
                    Ylovesc.style.background = "#F8F8F8";
                    Ylovesc.style.color = "#999";
            }
        });
    };
    //点击遮罩关闭
//    $('#dialogYc').bind('click',function(e){
//        var target = e.target;
//        if(target.closest('.dialog-info')==null){
//            $('#dialogYc').css("display","none");
//        };
//    });
    function qualified(){
        document.getElementById("dialogYc").style.display = "block";
    }
    function Yqualified(){
        var day =formatTimeToDay_day(Date.parse(document.getElementById('Time').innerHTML));
        ajaxGet(DayCheckgetaddClazzCheck,{cid:cid,day:day,index:0},function(ret,err){

            if(ret.success == true){
                toast("设置成功！");
                document.getElementById("dialogYc").style.display = "none";
                var Ylovesc = document.getElementById("Ylovesc");
                Ylovesc.setAttribute("disabled", true);
                Ylovesc.style.background = "#F8F8F8";
                Ylovesc.style.color = "#999";
            }else{
                toast("设置失败，请检查网络重新设置！");
                document.getElementById("dialogYc").style.display = "none";
            }
        })
    }
    //时间选择
    function formatTime(date){
        var year = date.getFullYear();
        var month = date.getMonth()+1, month = month < 10 ? "0" + month : month;
        var day = date.getDate(), day =day < 10 ? "0" + day : day;
        return year + '-' + month + '-' + day;
    }
    var now = new Date();  //获取当前日期
    var Time =document.getElementById("Time");
    var Week =document.getElementById("week");
    //初始化当前日期
    Time.innerHTML=formatTime(now);
    //初始化星期
    CalculationWeek(now.getDay());
    //减去
    function Subtract(){
        var arr = Time.innerHTML;
        var dd = new Date(arr);
        Time.innerHTML = addDate(dd,-1);
        //重置星期
        var Ctime = Time.innerHTML;
        var week = new Date(Ctime).getDay();
        CalculationWeek(week);
        var Yangtime = Date.parse(Ctime);
        var Yday = formatTimeToDay_day(Date.parse(document.getElementById('Time').innerHTML));

        ajaxGet(getClazzAllDayCheckByDay,{cid:cid,day:Yday},function(ret,err){

            if(ret.success ==false){
                var Ylovesc = document.getElementById("Ylovesc");
                Ylovesc.setAttribute("disabled", true);
                Ylovesc.style.background = "#F8F8F8";
                Ylovesc.style.color = "#999";
            }else{
                var Ylovesc = document.getElementById("Ylovesc");
                Ylovesc.removeAttribute("disabled");
                Ylovesc.style.background = "#5AC8E1";
                Ylovesc.style.color = "#fff";
            }
        });
    }
    //增加
    function increase(){
        var arr = Time.innerHTML;
        var dd = new Date(arr);
        Time.innerHTML = addDate(dd,+1);
        //重置星期
        var Ctime = Time.innerHTML;
        var week = new Date(Ctime).getDay();
        CalculationWeek(week);
        var Yangtime = Date.parse(Ctime);
        var Yday = formatTimeToDay_day(Date.parse(document.getElementById('Time').innerHTML));
        ajaxGet(getClazzAllDayCheckByDay,{cid:cid,day:Yday},function(ret,err){

            if(ret.success == false){
                var Ylovesc = document.getElementById("Ylovesc");
                Ylovesc.setAttribute("disabled", true);
                Ylovesc.style.background = "#F8F8F8";
                Ylovesc.style.color = "#999";
            }else{
                var Ylovesc = document.getElementById("Ylovesc");
                Ylovesc.removeAttribute("disabled");
                Ylovesc.style.background = "#5AC8E1";
                Ylovesc.style.color = "#fff";
            }
        });

    }
    //计算当前日期
    function addDate(date,days){
        var d=new Date(date);
        d.setDate(d.getDate()+days);
        var m=d.getMonth()+1;
        var mm;
        if(m<10){
            mm = "0" + m
        }
        var dd;
        if(d.getDate() < 10){
            dd = "0"+ d.getDate()
        }else{
            dd = d.getDate()
        }
        return d.getFullYear()+'-'+mm+'-'+dd;
    }
    //计算当前星期几并赋值
    function CalculationWeek(week){
        switch(week)
        {
            case 1:
                Week.innerHTML = "星期一";
                break;
            case 2:
                Week.innerHTML = "星期二";
                break;
            case 3:
                Week.innerHTML = "星期三";
                break;
            case 4:
                Week.innerHTML = "星期四";
                break;
            case 5:
                Week.innerHTML = "星期五";
                break;
            case 6:
                Week.innerHTML = "星期六";
                break;
            case 0:
                Week.innerHTML = "星期日";
                break;
        }
    }
</script>
</html>