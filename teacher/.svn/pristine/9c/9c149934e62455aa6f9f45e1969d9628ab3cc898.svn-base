<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>今日</title>
    <meta name="viewport" content="width=device-width, initial-scale=1,maximum-scale=1,user-scalable=no">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <link rel="stylesheet" type="text/css" href="../../css/api.css" />
    <link rel="stylesheet" type="text/css" href="../../css/commonWindow.css" />
    <style>
        html,
        body {
            width: 100%;
            height: 100%;
        }
        /**========底部菜单（开始）=====**/

        #nav {
            background: #FFFFFF;
            box-shadow: 0 0 5px 0 rgba(0, 0, 0, 0.20);
            width: 100%;
            text-align: center;
            position: relative;
            font-size: 10px;
            height: 49.6px;
        }

        #nav ul {
            display: -webkit-box;
            display: -webkit-flex;
            display: flex;
            -webkit-box-orient: horizontal;
            -webkit-flex-direction: row;
            flex-direction: row;
        }

        #nav ul li {
            -webkit-box-flex: 1;
            -webkit-flex: 1;
            flex: 1;
        }

        #nav li a {
            color: #a8a8a8;
            display: block;
            padding-top: 29px;
            line-height: 14px;
            font-size: 10px;
        }

        #nav li a:nth-child(1) {
            -webkit-background-size: 18px 18.5px !important;
            background-size: 18px 18.5px !important;
        }

        #nav li a:nth-child(2) {
            -webkit-background-size: 23px 23px !important;
            background-size: 23px 23px !important;
        }

        #nav li a:nth-child(3) {
            -webkit-background-size: 17px 18px !important;
            background-size: 17px 18px !important;
        }

        #nav li a:nth-child(4) {
            -webkit-background-size: 19px 17px !important;
            background-size: 19px 17px !important;
        }

        #nav li.active a,
        #nav a.active {
            color: #5AC8E1;
        }

        #nav .user {
            background: url(../../icon/home/mine.png) no-repeat center 6px;
        }

        #nav .user.active,
        #nav li.active .user {
            background: url(../../icon/home/mine_01.png) no-repeat center 6px;
        }

        #nav .wonderfulTime {
            position: relative;
            background: url(../../icon/home/home.png) no-repeat center 6px;
        }

        #nav .wonderfulTime.active,
        #nav li.active .wonderfulTime {
            background: url(../../icon/home/home_01.png) no-repeat center 6px;
        }

        #nav .msg {
            background: url(../../icon/home/ourSchool_01.png) no-repeat center 6px;
        }

        #nav .msg.active,
        #nav li.active .msg {
            background: url(../../icon/home/ourSchool.png) no-repeat center 6px;
        }

        #nav .home {
            background: url(../../icon/home/cute.png) no-repeat center 6px;
        }

        #nav .home.active,
        #nav li.active .home {
            background: url(../../icon/home/cute_01.png) no-repeat center 6px;
        }

        #header {
            width: 100%;
            height: 44px;
            background: #5AC8E1;
            line-height: 44px;
            color: #fff;
        }

        #header .header {
            position: relative;
            width: 100%;
            height: 44px;
            display: none;
            position: relative;
            text-align: center;
            font-size: 18px;
        }

        #header .active {
            display: block;
        }

        .parFind {
            width: 40px;
            height: 44px;
            position: absolute;
            top: 0;
            right: 38px;
        }

        .header .img {
            position: absolute;
            bottom: 14px;
            right: 20px;
            width: 16px;
            height: 18px;
        }

        .header .point {
            display: none;
            position: absolute;
            bottom: 30px;
            right: 15px;
            width: 7px;
            height: 7px;
            border-radius: 100%;
            background: #e60012;
        }

        .ulList_01 li,
        .ulList li {
            width: 33.33%;
            height: 52px;
            line-height: 52px;
            font-size: 14px;
            float: left;
            overflow: hidden;
            border-bottom: 1px solid #ccc;
        }

        .ulList_01 li {
            width: 25.00%;
        }

        .ulList .actives {
            color: #31c27c;
        }

        .color {
            color: #333;
        }

        .applyOrders {
            position: absolute;
            right: 0;
            bottom: 0;
            float: right;
            width: 92px;
            height: 48px;
            line-height: 48px;
            text-align: center;
            background: #7b7b7b;
            color: #fff;
            font-size: 16px;
            text-indent: 4px;
        }

        .applyOrders img {
            width: 10px;
            height: 10px;
            position: absolute;
            top: 18px;
            left: 4px;
        }

        #move_02,
        #move_01,
        #move {
            width: 25%;
            height: 2px;
            background: #31c27c;
            position: absolute;
            left: 4.16%;
            bottom: 0;
        }

        #move_02,
        #move_01 {
            width: 20%;
            left: 2.5%
        }

        .nav {
            width: 100%;
            line-height: 44px;
            background: #fff;
            margin: 1px 0 6px 0;
            overflow: hidden;
            border-top: 1px solid #ebebeb;
        }

        .nav li {
            width: 25.00%;
            height: 44px;
            line-height: 44px;
            font-size: 12px;
            float: left;
            text-align: center;
        }

        .ulList_03 li {
            width: 25.00%;
            font-size: 15px;
        }

        .navList {
            color: #333;
        }

        .ulList_03 .active {
            color: #31c27c;
        }

        #header_01 {
            width: 95.08%;
            background: #fff;
            margin: 0 auto;
            overflow: hidden;
        }

        #header_01 .list {
            width: 22.30%;
            height: 30px;
            line-height: 30px;
            text-align: center;
            color: #999;
            font-size: 15px;
            border: 1px solid #ccc;
            float: left;
            margin-top: 13px;
            margin-left: 3.1%;
            border-radius: 15px;
        }

        #header_01 .list:nth-child(4n-3) {
            margin-left: 0;
        }

        #header_01 .list:nth-child(4n) {
            margin-left: 0;
            float: right;
        }

        .swiper-container-horizontal>.swiper-pagination-bullets,
        .swiper-pagination-custom,
        .swiper-pagination-fraction {
            bottom: -15px;
        }

        .swiper-pagination-bullet-active {
            background: #31c27c;
        }
        .right{
            right: 0px;
            position: absolute;
            width: 60px;
            height: 44px;
            line-height: 44px;
            z-index: 10;
            bottom: 0;
            align-items: center;
            display: flex;
            justify-content: center;
        }
        .right img{
            right: 20px;
            width: 30px;
            height: 30px;
        }
    </style>
</head>

<body>
    <div id="wrap">
        <div id="header">
            <div class="home header" style="position: relative;">
                <span class="name"></span>
            </div>
            <div class="wonderfulTime header" style="position: relative;">
                    <div id="leftButton" onclick="openwonderfulTime_examine()">
                        <img src="../../image/shenhe.png" style="width:23px;height:23px;margin-left:5px;">
                    </div>
                    <div onclick="Topselect(this)"><span id="titleText">全班</span><img src="../../image/arrow_bt_white.png" alt="" class="arrow_btm"/></div>
                    <div onclick="openwonderfulTime_release()" class="right"><img style="width:21px;height:21px;" src="../../icon/addicon.png"/></div>
            </div>
            <div class="msg header" style="position: relative;">
                <span id="MeKing" class=""></span>
            </div>
            <div class="user header" style="height:0;">

            </div>
        </div>
        <div id="main" style="overflow: auto;">
        </div>
        <div id="nav">
            <ul>
                <li>
                    <a class="home" tapmode="active" onclick="openTab('home')">智慧乐园</a>
                </li>
                <li style="position: relative;">
                    <a class="wonderfulTime" tapmode="active" onclick="openTab('wonderfulTime')">精彩瞬间</a>
                </li>
                <li>
                    <a class="msg" tapmode="active" onclick="openTab('msg')">我们班</a>
                </li>
                <li>
                    <a class="user" tapmode="active" onclick="openTab('user')">我</a>
                </li>
            </ul>
        </div>
    </div>
    <script type="text/javascript" src="../../script/jquery-1.8.1.min.js"></script>
    <script type="text/javascript" src="../../script/public.js"></script>
    <script type="text/javascript" src="../../script/init.js"></script>
    <script type="text/javascript" src="../../script/api.js"></script>
    <script type="text/javascript" src="../../script/user.js"></script>
    <script type="text/javascript" src="../../script/kv.js"></script>
    <script type="text/javascript" src="../../script/apiCloud.js"></script>
    <script type="text/javascript" src="../../script/template-native.js"></script>
    <script type="text/javascript" src="../../script/data.js"></script>
    <script>
        apiready = function() {
        	api.setStatusBarStyle({
	    		style:"dark",
	    		color:"rgba(0,0,0,0)"
			});
            $api.setStorage("frameOpened_wonderfulTimeSelect_body","");//打开切换精彩瞬间切换页面的时候   存在本地的标示  需要清除
            var headName = document.getElementsByClassName('name');
            headName[0].innerHTML = getUserInfo().schoolName;
            setTitle_className();
            var statusBarAppearance = api.statusBarAppearance;//是否支持沉浸式
            if (statusBarAppearance){
                var header = $api.dom('#header');
                $api.fixStatusBar(header);
            }
            // 解决iphone X底部高度兼容
            $api.fixTabBar($api.byId('nav'));
            //配置tab选项卡 各个页面
            tabs = {
                home: {
                    isGroup: false,
                    frame: {
                        name: 'home',
                        url: './home_body.html',
                        pageParam: {},
                        rect: {
                            x: 0,
                            y: headHeight + api.safeArea.top,
                            w: api.frameWidth,
                            h: api.winHeight - bottomHeight - api.safeArea.top - headHeight - api.safeArea.bottom - 4
                        },
                        bounces: false,
                        opaque: false,
                        //bgColor:'#EDEDED',
                        bgColor: '#fff',
                        allowEdit: true,
                        vScrollBarEnabled: false,
                        showProgress: false,
                        hScrollBarEnabled: false,
                        reload: false
                    }
                },
                wonderfulTime: {
                    isGroup: false,
                    frame: {
                        name: 'wonderfulTime',
                        url: '../wonderfulTime/wonderfulTime_body.html',
                        pageParam: {},
                        rect: {
                            x: 0,
                            y: headHeight + api.safeArea.top,
                            w: api.frameWidth,
                            h: api.winHeight - bottomHeight - api.safeArea.top - headHeight - api.safeArea.bottom - 4
                        },
                        bounces: false,
                        opaque: false,
                        //bgColor:'#EDEDED',
                        bgColor: '#fff',
                        allowEdit: true,
                        vScrollBarEnabled: false,
                        showProgress: false,
                        hScrollBarEnabled: false,
                        reload: false
                    }
                },
                msg: {
                    isGroup: false,
                    frame: {
                        name: 'msg',
                        url: 'ourClass_body.html',
                        pageParam: {},
                        rect: {
                            x: 0,
                            y: headHeight + api.safeArea.top,
                            w: api.frameWidth,
                            h: api.winHeight - bottomHeight - api.safeArea.top - headHeight -
                            api.safeArea.bottom - 4
                        },
                        bounces: false,
                        opaque: false,
                        //bgColor:'#EDEDED',
                        bgColor: '#fff',
                        allowEdit: true,
                        vScrollBarEnabled: false,
                        showProgress: false,
                        hScrollBarEnabled: false,
                        reload: false
                    }
                },
                user: {
                    isGroup: false,
                    frame: {
                        name: 'user',
                        url: '../mine/mine.html',
                        pageParam: {},
                        rect: {
                            x: 0,
                            y: api.safeArea.top,
                            w: api.frameWidth,
                            h: api.winHeight - bottomHeight - api.safeArea.top -
                            api.safeArea.bottom - 4
                        },
                        bounces: false,
                        opaque: false,
                        //bgColor:'#EDEDED',
                        bgColor: '#fff',
                        allowEdit: true,
                        vScrollBarEnabled: false,
                        showProgress: false,
                        hScrollBarEnabled: false,
                        reload: false
                    }
                }
            }

            //		systemInit();

            //添加安卓退出按钮事件
            api.addEventListener({
                name: 'keyback'
            }, function(ret, err) {
                closeOnKeyBack();
            });

            openTab('home');
        };

        function zlHandsome(title,type){
            document.getElementById("titleText").innerHTML = title;
        }

        var backButtonPress = 0;
        function toggleAction(){
            action = false
        }
        var action = true;
        var type = true;
        function Topselect(){
            if(type == true){
                api.openFrame({
                    name: 'cesda',
                    url: '../wonderfulTime/wonderfulTimeSelect_body.html',
                    rect: {
                        x: 0,
                        y: headHeight + api.safeArea.top,
                        w: 'auto',
                        h: api.winHeight - bottomHeight - api.safeArea.top - headHeight - api.safeArea.bottom
                    },pageParam: {
                        type: '0'
                    }
                });
                type = false;
                $api.setStorage("frameOpened_wonderfulTimeSelect_body",true);//本地存储一个变量 标示这个frame打开了
            }else{
                if(action == true){
                    api.setFrameAttr({
                        name:"cesda",
                        hidden:true
                    });
                    action = false;
                }else{
                    api.setFrameAttr({
                        name:"cesda",
                        hidden:false
                    });
                    action = true;
                }
            }
        }
        //打开精彩瞬间审核
        function openwonderfulTime_examine(){
            api.setFrameAttr({
                name:"cesda",
                hidden:true
            });
            action = false;
            openNewWindow("wonderfulTime_examine","../wonderfulTime/wonderfulTime_examine.html")
        }
        //打开精彩瞬间发布
        function openwonderfulTime_release(){
            api.setFrameAttr({
                name:"cesda",
                hidden:true
            });
            action = false;
            openNewWindow("wonderfulTime_release","../wonderfulTime/wonderfulTime_release.html")
        }

        function closeOnKeyBack() {
            var PhotoBrowser_flag = $api.getStorage("PhotoBrowser_flag");
            if (PhotoBrowser_flag) {
                api.execScript({
                    name:'main',
                    frameName : "wonderfulTime",
                    script: "closePhotoBrowser()"
                });
                return;
            }
            backButtonPress++;
            if (backButtonPress > 1) {
                closeApp();
            } else {
                api.toast({msg: '再按一次退出应用'});
            }
            setTimeout(function () {
                backButtonPress = 0;
            }, 1000);
        }



        //设置第三个frame页面的班级名称
        function setTitle_className(){
            if(getUserInfo().clazzes == '' || getUserInfo().clazzes == NaN){
                toast('暂无班级')
            }else{
                document.getElementById("MeKing").innerHTML = getUserInfo().clazzes[0].name;
            }
        }
    </script>
</body>

</html>
