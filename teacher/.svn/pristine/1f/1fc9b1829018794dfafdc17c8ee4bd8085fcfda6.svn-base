	<script type="text/html" id="listScript">
		<%if(list.length){%>
			<div class="rd-info">
                <table>
                    <thead>
                        <tr class="top-tab">
                            <th>服务设备</th>
                            <th>数量</th>
                            <th>班级</th>
                            <th>分配时间</th>
                        </tr>
                    </thead>
                    <tbody>
						<%for(var i=0;i<list.length;i++){%>
                        <tr class="rd-content">
                            <td class="good-name"><%=list[i].name%></td>
                            <td class="good-count"><%=list[i].num%></td>
                            <td class="good-units"><%=list[i].clazzName%></td>
                            <td class="good-date"><%=list[i].Utime%></td>
                        </tr>
						<%}%>
					</tbody>
				</table>
			</div>
		<%}%>
	</script>
	<script>
		apiready = function(){
			var Sid = getUserInfo().sid;
//            alert(Sid);
			new datalistModel(addDataToHtml,getDistributions,{sid:Sid},10,5,100,true);
		};
		function addDataToHtml(results){
			for(var i=0;i<results.length;i++){
				var a = formatTimeToDay(results[i].utime);
				results[i]['Utime'] = a;
			}
			var data = {list: results};
			var html = template('listScript',data);
			document.getElementById('listDom').innerHTML = html;
			api.parseTapmode();
			document.getElementById("body").style.display = "block";
		}
	</script>
