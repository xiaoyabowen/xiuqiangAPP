            text-align: right;
        }

        thead > tr > :nth-child(1) {
            padding-left: 3px;
        }

        thead > tr > :nth-child(4) {
            padding-right: 3px;
        }

        tbody tr td {
            font-size: 12px;
            color: #333333;
        }

        tbody tr > :nth-child(1) {
            text-align: left;
            padding-left: 3px;
        }

        tbody tr > :nth-child(4) {
            padding-right: 4px;
        }

        tbody tr > :nth-child(2) {
            text-align: center;
            padding-left: 40px;
        }

        tbody tr > :nth-child(3),
        tbody tr > :nth-child(4) {
            text-align: right;
        }

        tbody tr > :nth-child(2) {
            padding-right: 2px;
        }

        tbody tr {
            width: 100%;
            height: 41px;
            line-height: 41px;
            border-bottom: 1px solid #F2F2F2;
        }

        tbody > :nth-last-child(1) {
            border: none;
        }

        tbody tr .unnormal > span {
            width: 46px;
            height: 21px;
            background: #D26034;
            color: #FFFFFF;
            border-radius: 4px;
            line-height: 21px;
        }
