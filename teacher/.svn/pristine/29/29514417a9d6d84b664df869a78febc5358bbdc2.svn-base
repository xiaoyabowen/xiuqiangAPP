        }

        .detail-list {
            width: 100%;
            margin-top: 20px;
            text-align: center;
            box-sizing: border-box;
        }

        .clockin-record {
            width: 100%;
            margin-top: 20px;
        }

        .cr-title {
            width: 100%;
            height: 52px;
            background: #EBFBFF;
            text-align: center;
            line-height: 52px;
            font-size: 16px;
            color: #505050;
