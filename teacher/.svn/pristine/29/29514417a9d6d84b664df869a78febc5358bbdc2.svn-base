        }

        .cr-title span {
            position: relative;
            padding-left: 33px;
        }

        .cr-title img {
            position: absolute;
            width: 21px;
            height: 21px;
            left: 0px;
            top: 15px;
        }

        .clocking-table {
            padding-left: 15px;
            padding-right: 15px;
            margin-top: 20px;
            box-sizing: border-box;
        }

        table {
            width: 100%;
        }

        thead > tr {
            width: 100%;
        }

        thead > tr th {
            width: 25%;
            font-weight: normal;
            font-size: 14px;
            color: #505050;
        }

        thead > tr > :nth-child(1) {
            text-align: left;
        }

        thead > tr > :nth-child(1) > img {
            width: 7px;
            vertical-align: middle;
            margin-left: 30px;
        }

        thead > tr > :nth-child(2) {
            text-align: center;
