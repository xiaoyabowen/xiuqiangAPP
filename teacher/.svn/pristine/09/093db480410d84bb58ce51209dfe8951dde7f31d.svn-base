		<script src="https://cdn.bootcss.com/eruda/1.2.6/eruda.js"></script>
		<script>eruda.init();</script>
		<style type="text/css">
			html,body{
				width: 100%;
				height: 100%;
				min-width: 320px;
				background-color: #f8f8f8;
			}
			.tp_user{
				position: relative;
				height: 148px;
				width: 100%;
				background: url(../../image/mebg.png);
				background-repeat-y: repeat;
				background-size: cover;
				/*line-height: 130px;*/
				position: relative;
				padding:0 18px;
				box-sizing: border-box;
				overflow: hidden;
			}
			.shiftClass{
				font-size: 16px;
				color: #FFFFFF;
				letter-spacing: 0.22px;
				text-align: right;
			}
			.shiftClass em{
				margin-top: 30px;
				/*height: 22px;*/
				box-sizing: border-box;
				/*line-height: 26px;*/
				padding: 2px 0;
				background: #5AC8E1;
				text-align: center;
				border-radius: 4px;
				padding-left: 37px;
				position: relative;
				padding-right: 10px;
			}
			.shiftClass em:before{
				content: '';
				width: 14px;
				height: 15px;
				background: url(../../image/ringrotate.png) no-repeat;
				position: absolute;
				left: 10px;
				top: 50%;
				margin-top: -7px;
			}
			.userCon{
				float: left;
				display: inline-block;
				width: 86px;
				height: 86px;
				border-radius: 43px;
				padding: 3px;
				box-sizing: border-box;
				background: #fff;
				overflow: hidden;
			}
			#name{
				float: left;
				margin-left: 12px;
				line-height: 86px;
			}
			.con{
				position: absolute;
				left: 18px;
				bottom: 15px;
				width: 100%;
				height: 86px;
				/*line-height: 86px;*/
				font-size: 16px;
				color: #484848;
				letter-spacing: 0.22px;
			}

			.userCon {
				float: left;
				display: inline-block;
				width: 80px;
				height: 80px;
				border-radius: 50%;
				border: 3px solid #fff;
				background: #fff;
				padding: 0 !important;
				overflow: hidden;
			}
			/*头像样式*/
			#userIcon {
				display: block;
			}
			.operationList{
				background-color: #fff;
				margin-top:12px;
			}
			.operationList li,.other_operationList li{
