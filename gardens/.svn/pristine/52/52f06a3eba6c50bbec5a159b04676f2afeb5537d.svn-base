<!DOCTYPE html>
<html>
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1,maximum-scale=1,user-scalable=no">
	<meta name="apple-mobile-web-app-capable" content="yes">
	<meta name="apple-mobile-web-app-status-bar-style" content="black">
	<title>精彩瞬间</title>
	<link rel="stylesheet" href="../../css/api.css" />
	<link rel="stylesheet" href="../../css/commonWindow.css" />
	<style type="text/css">
		body{
			min-width: 320px;
		}
		.arrow_btm{
			padding-left: 7px;
			padding-bottom: 2px;
		}
		#topbar .right{
			right: 15px;
		}
		#topbar .right img{
			right: 20px;
			width: 30px;
			height: 30px;
		}

	</style>
</head>
<body>
<div id="topbar" >
	<div id="leftButton" onclick="closeWin()">
		<img style="width: 30px;height: 30px;" src="../../icon/back.png">
	</div>
	<div><span id="titleText"></span></div>
</div>
</body>
<script type="text/javascript" src="../../script/api.js"></script>
<script type="text/javascript" src="../../script/mui.min.js"></script>
<script type="text/javascript" src="../../script/util.js"></script>
<script type="text/javascript" src="../../script/kv.js"></script>
<script type="text/javascript" src="../../script/apiCloud.js"></script>
<script type="text/javascript" src="../../script/public.js"></script>
<script type="text/javascript" src="../../script/template-native.js"></script>
<script type="text/javascript" src="../../script/data.js"></script>
<script>
	apiready = function () {
		initFrameInWin(api.pageParam);
		document.getElementById("titleText").innerHTML = api.pageParam.content;
	};
	function toggleAction(name){
		if(name != document.getElementById("titleText").innerHTML){
			document.getElementById("titleText").innerHTML = name;
		}
	}
</script>
</html>
