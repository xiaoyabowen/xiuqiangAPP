
<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<meta name="viewport" content="width=device-width, initial-scale=1,maximum-scale=1,user-scalable=no">
    	<meta name="apple-mobile-web-app-capable" content="yes">
    	<meta name="apple-mobile-web-app-status-bar-style" content="black">
		<title>宝宝健康</title>
		<link rel="stylesheet" href="../../css/api.css" />
		<link rel="stylesheet" href="../../css/commonWindow.css" />
		<style type="text/css">
			body{
				min-width: 320px;
                background: #F2F2F2
			}
            .select-date{
                width:100%;
                height:42px;
                padding-left:8px; 
                padding-right:8px;
                padding-top:8px;
                box-sizing: border-box;
            }
            .select-date .sd{
                width:100%;
                height:42px;
                background: #FFFFFF;
                box-shadow: 0 2px 4px 0 #CDE3E9;
                border-radius: 4px;
                position: relative;
                text-align: center;
            }
            .sd .left-btn>img, .sd .right-btn>img{
                width:16px;
                height:16px;
            }
            .sd .left-btn{
                position:absolute;
                left:15px;
                top:13.5px;
            }
            .sd .right-btn{
                position:absolute;
                right:15px;
                top:13.5px;
            }
            .sd .show-time{
                font-size: 16px;
                color: #333333;
                line-height:42px;
            }
            .pie-chart{
                width:100%;
                padding-left:8px;
                padding-right:8px;
                margin-top:20px;
                box-sizing: border-box;
            }
            .pc{
                width:100%;
                height:154px;
                background: #FFFFFF;
                box-shadow: 0 2px 4px 0 #CDE3E9;
                border-radius: 4px;
                padding-top:20px;
                padding-left:18px;
                position: relative;
                box-sizing: border-box;                
            }
            .pc>canvas{
                width:234px;
                height:117px;
            }
            .blank{
                width:50px;
                height:50px;
                background: #FFFFFF;
                border-radius:50%;
                position:absolute;
                top:53px;
                left:53px;
            }
            .right-tag{
                position: absolute;
                top:35px;
                left:167px;
            }
            .normal>:nth-child(1){
                width:16px;
                height:16px;
                background: #67C49C;
                box-shadow: 0 2px 4px 0 rgba(155,218,191,0.50);
                border-radius: 2px;
            }
            .normal{
                position:relative;
            }
            .unnormal{
                margin-top:16px;
                position:relative;                
            }
            .unnormal-rate{
                margin-top:9px;
                position:relative;           
            }
            .unnormal>:nth-child(1){
                width:16px;
                height:16px;
                background: #D4602C;
                box-shadow: 0 2px 4px 0 rgba(155,218,191,0.50);
                border-radius: 2px;
            }
            .unnormal-rate>:nth-child(1){
                width:16px;
                height:16px;
                background: #FFFFFF;
            }
            .normal>:nth-child(1),.unnormal>:nth-child(1),.unnormal-rate>:nth-child(1){
                position: absolute;
                top:3px;
            }
            .normal>:nth-child(2),.unnormal>:nth-child(2),.unnormal-rate>:nth-child(2){
                margin-left:28px;
                font-size:12px;
                color:#333333;
            }
            .normal>:nth-child(3),.unnormal>:nth-child(3),.unnormal-rate>:nth-child(3){
                margin-left:12px;
                font-size:16px;
                color:#151515;
            }
            .detail-list{
                width:100%;
                padding:12px 8px 19px 8px;
                box-sizing: border-box;
            }
            .dl{
                width:100%;
                background: #FFFFFF;
                box-shadow: 0 2px 4px 0 #CDE3E9;
                padding:20px 18px 22px;
                border-radius: 4px;
                box-sizing: border-box;
            }
            .dl-title ul li{
                width:50%;
                height:31px;
                font-size: 16px;
                color: #999999;
                float:left;
                text-align: center;
                box-sizing: border-box;
            }
            .dl-title ul .active{
                color:#5EC4E0 !important;
            }
            .dl-title ul>:nth-child(1){
                border-right:1px solid #E8E7E7;
            }
            .dl-info{
                clear:left;
                margin-top:8px;
                box-sizing: border-box;                
            }
            .dl-info ul{
                width:100%;
                box-sizing: border-box;  
            }
            .dl-info ul li{
                width:100%;
                height:46px;
                border-bottom:1px solid #E8E7E7;
                font-size: 14px;
                color:#333333;
                padding-top:20px;
                box-sizing: border-box;
            }      
		</style>
	</head>
	<body>
        <div class="select-date">
            <div class="sd">
                <div class="left-btn"><img src="../../icon/arrowPre.png" alt=""></div>
                <div class="show-time">
                    <span class="year-month">2018-02-17</span>
                    <span class="week">星期二</span>
                </div>
                <div class="right-btn"><img src="../../icon/arrowNext.png" alt=""></div>                    
            </div>
        </div>
        <div class="pie-chart">
            <div class="pc">
                <canvas id="data-chart"></canvas>
                <div class="blank"></div>
                <div class="right-tag">
                    <div class="normal"><span></span><span>正常</span><span>85</span></div>
                    <div class="unnormal"><span></span><span>异常</span><span>15</span></div>
                    <div class="unnormal-rate"><span></span><span>异常率</span><span>15%</span></div>
                </div>              
            </div>
        </div>
        <div class="detail-list">
            <div class="dl">
                <div class="dl-title">
                    <ul>
                        <li class="active">未见异常<span>（85）</span></li>
                        <li>发现异常<span>（15）</span></li>
                    </ul>
                </div>
                <div class="dl-info">
                    <ul>
                        <li><span>张丽珍</span></li>
                        <li><span>张丽珍</span></li>
                        <li><span>张丽珍</span></li>
                        <li><span>张丽珍</span></li>
                        <li><span>张丽珍</span></li>
                        <li><span>张丽珍</span></li>
                        <li><span>张丽珍</span></li>
                        <li><span>张丽珍</span></li>
                        <li><span>张丽珍</span></li>
                        <li><span>张丽珍</span></li>
                        <li><span>张丽珍</span></li>         
                    </ul>
                </div>
            </div>
        </div>
	</body>
	<script type="text/javascript" src="../../script/api.js"></script>
	<script type="text/javascript" src="../../script/util.js"></script>
	<script type="text/javascript" src="../../script/kv.js"></script>
	<script type="text/javascript" src="../../script/apiCloud.js"></script>
	<script type="text/javascript" src="../../script/public.js"></script>
	<script type="text/javascript" src="../../script/template-native.js"></script>
    <script type="text/javascript" src="../../script/data.js"></script>
    <script>
     var drawCircle = function(canvasId, data_arr, color_arr){
            var drawing = document.getElementById(canvasId);
            if(drawing.getContext){
                var context = drawing.getContext('2d');
                var radius = 75,//半径
                    ox = radius, oy = radius; //圆心
                var startAngle = 0, endAngle = 0;//起始、结束弧度
                for(var i=0,len=data_arr.length;i<len;i++){
                    //绘制饼图
                    endAngle += data_arr[i]*2*Math.PI;
                    context.fillStyle = color_arr[i];
                    context.beginPath();
                    context.moveTo(ox, oy);
                    context.arc(ox, oy, radius, startAngle, endAngle, false);
                    context.closePath();
                    context.fill();
                    startAngle = endAngle;
                }
            }			
        };
        var init = function(){
            var data_arr = [0.85, 0.15],
                color_arr = ['#67C49C','#D4602C'];
            drawCircle('data-chart', data_arr, color_arr);
        };
        init();
    </script>
</html>