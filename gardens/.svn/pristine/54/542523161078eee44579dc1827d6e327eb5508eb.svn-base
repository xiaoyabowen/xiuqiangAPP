    <title>宝宝食谱</title>
    <style>
        body{
            background: #F2F2F2;
            height:auto;
        }
        .select-menu{
            width:100%;
            margin-top:12px;
            padding-left:8px;
            padding-right:8px;
            box-sizing: border-box;
        }
        .select-menu .sm{
            width:100%;
            height:42px;
            background: #FFFFFF;
            box-shadow: 0 2px 4px 0 #CDE3E9;
            border-radius: 4px;
            padding-left:18px;
            padding-top:9px;
            box-sizing: border-box; 
            display: flex;
            justify-content: center;
            position: relative;
        }
        .select-version{
            height:24px;
            line-height:24px;
            background: #92CA55;
            color:#FFFFFF;
            font-size: 12px;
            border-radius: 100px;
            padding-left:10px;
            padding-right:10px;            
            display: inline-block;
            box-sizing: border-box;
            position: absolute;
            left:18px;
        }
        @media screen and (max-width: 320px) {
            .select-version{
                left:5px;                
            }
        }
        .select-version .version-name::after{
            content:url('../../icon/arrowup.png');
            width:12px;
            height:12px;
            vertical-align: middle;
            margin-left:7px;
        }
       
        .select-date{
            display: inline-block;            
            height:24px;
            line-height:24px;            
            /* margin:0 auto; */
            font-size: 12px;
            color: #FFFFFF;
            padding-left:13px;
            padding-right:13px;
            background: #67C49C;
            border-radius: 100px;
            box-sizing: border-box;         
        }
        .select-date div{
            float:left;
        }
        .start-date,.end-date{
            text-decoration: underline;            
        }
        .cookbook-info{
            width:100%;
            margin-top:12px;
            padding-left:8px;
            padding-right:8px;
            padding-bottom:18px;
            box-sizing: border-box;
        }
        .cookbook-info .ci{
            width:100%;
            background: #FFFFFF;
            box-shadow: 0 2px 4px 0 #CDE3E9;
            border-radius: 4px;
            padding:20px 18px; 
            padding-left:18px;
            padding-top:20px;
            padding-bottom:18px;
            box-sizing: border-box; 
            position: relative;
        }
        .cookbook-info .ci .edit{
            position:absolute;
            right:18px;
            top:19px;
        }
        .cookbook-info .ci .edit>img{
            width:22px;
            height:22px;
        }
        .ci-title div{
            float:left;
        }
        .ci-menu{
            padding-top:11px;
        }
        .ci-content{
            clear:left;
            margin-bottom:17px;
        }
        .ci-title{
            height:22px;
        }
        .ci-title .ci-name{
            font-size: 16px;
            color: #333333;
            line-height:22px;          
        }
        .ci-title .ci-time{
            font-size:12px;
            color:#999999;
            line-height: 20px;
            margin-left:15px;
        }
        .cil-left{
            clear:left;
        }
        .cil-left img{
            float:left;
            width:105px;
            height:105px;
            border-radius:4px;
        }
        .cil-right{
            width:100%;
            height:105px;
            padding-left:117.5px;
            box-sizing: border-box;            
        }
        .cil-right .cr-item{
            margin-bottom:10px;
            font-size: 14px;
            color: #333333;
            line-height:20px;
            box-sizing: border-box;            
        }
        .cil-right .cr-item>:nth-child(2){
            margin-left:-5px;          
        }
        .breakfast{
            padding-bottom:3px;
        }
        .lunch{
            padding-bottom:3px;            
        }
        .dinner{
            padding-bottom:3px;            
        }
        .e-title{
            font-size: 14px;
            color: #333333;
        }
        .e-text{
            font-size: 12px;
            color: #999999;
            line-height: 17px;
            margin-top:18px;
            padding-right:18px;
            box-sizing: border-box;
        }
