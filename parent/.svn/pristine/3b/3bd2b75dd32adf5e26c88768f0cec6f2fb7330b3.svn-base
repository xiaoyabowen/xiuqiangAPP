        //点击切换宝贝模板
        function changeShifts(){
            document.getElementById('changeShifts').style.display = 'block';
        }
        //关闭切换宝贝模板
        function closeZ(){
            document.getElementById('changeShifts').style.display = 'none';
        }
        
        
        //点击具体的班级  切换班级  刷新数据
		function chooseThisBady(id){
			var userInfo = getUserInfo();
			var list = userInfo.children;
			var list_id = userInfo.cids;
			var allResult = {};
			for (var i = 0; i < list.length; i++) {
				allResult[list[i].id] = list[i];
				if (list[i].id == id) {
					list.splice(i, 1);
					list_id.splice(i,1);
				}
			}

			list_id.unshift(id);
			list.unshift(allResult[id]);
			
			userInfo.children = list;
			userInfo.cids = list_id;
			setUserInfo(userInfo);
			api.execScript({
	        	name:'main',
	        	frameName : "wonderfulTime",
		        script: "apiready()"
		    });
		    
		    api.execScript({
	        	name:'main',
	        	frameName : "msg",
		        script: "apiready()"
		    });
            api.execScript({
                name:'main',

                script: "classNameCheck()"
            });
			setTimeout(function(){
				closeZ();
			},200)
		}
	</script>
