		<style type="text/css">
			html,body{
				width: 100%;
				height: 100%;
				min-width: 320px;
				background-color: #f9f8f8;
			}
			.mine{
				padding-top: 12px;
				background-color: #f8f8f8;
			}
			.schoolList{
				background-color: #fff;
				width: 100%;
				box-sizing: border-box;
			}
			.schoolList li{
				border-bottom: 1px solid #d9d9d9;
				height: 40px;
				line-height: 39px;
				margin-left: 19px;
				position: relative;
				font-size: 16px;
				color: #505050;
				letter-spacing: -1.63px;
			}

			.schoolList li span{
				margin-left: 36px;
				overflow: hidden;
				display: block;
				position: relative;
			}
			.schoolList li:nth-last-of-type(1) span{
				border-bottom: 0;
			}
			.schoolList li:before{
				content: '';
				height: 20px;
				width: 20px;
				background-size: 100%;
				position: absolute;
				left: 0;
				top: 50%;
				margin-top: -10px;
			}
			.mine01:before{
				/*width: 20px;*/
				background: url(../../image/ourclass01.png) no-repeat;
			}
			.mine02:before{
				/*width: 20px;*/
