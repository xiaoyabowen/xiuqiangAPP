    <script>
//  	apiready = function(){
    		var canvas = document.getElementById("cas");  
			var ctx= canvas.getContext("2d");
	  		var x0 = canvas.width/2;
			var y0 = canvas.height/2;
			var r0 = canvas.width/2-20;
			//绘制底部白色底框
	  		ctx.beginPath();
	    	var grad  = ctx.createLinearGradient(-Math.PI/2,-x0,90,x0+120);
			grad.addColorStop(0,'#f58c51');
			grad.addColorStop(0.5,'#d30404');
			grad.addColorStop(1,'#f58c51');
			ctx.strokeStyle = grad;
	    	ctx.lineWidth = 4;  
	    	ctx.arc(x0,y0,r0,0,2*Math.PI,false);  
			ctx.stroke();
	   		ctx.closePath(); 
	   		
	   		
//  	}
